{
    "name": "subsguru/payline-payment-plugin",
    "description": "Payline payment plugin for Subs.Guru",
    "type": "cakephp-plugin",
    "repositories": {
        "subsguru/core": {
            "type": "path",
            "url": "/Users/yves/Sites/core",
            "options": {
                "symlink": true
            }
        }
    },
    "require": {
        "php": ">=5.6.0",
        "subsguru/core": "dev-master",
        "commercie/currency": ">=1.1.0 <2.0.0",
        "monext/payline-sdk": ">=4.52 <5.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "SubsGuru\\Payline\\": "src\\"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SubsGuru\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    },
    "minimum-stability" : "dev",
    "prefer-stable" : true
}
